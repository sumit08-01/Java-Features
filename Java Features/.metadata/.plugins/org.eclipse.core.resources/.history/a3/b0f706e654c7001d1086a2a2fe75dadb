package com.core_java;


class A {
	
	public A() {
		System.out.println("non-parameterized A class");
	}
}

class B extends A {
	
	public B() {
		System.out.println("non-parameterized B class");
	}
}

public class Super_this_keyword{

	public static void main(String[] args) {
		B b = new B();
		
	}

}

package com.core_java;

class Emp{
	private String name;
	private int age;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
}

public class This_Keyword {

	public static void main(String[] args) {
		
		Emp emp = new Emp();
		emp.setName("sumit");
		emp.setAge(21);

	}

}

package com.core_java.thread;

class A {
	public void show() {
		for(int i=0; i<100; i++) {
			System.out.println("Hii");
		}
	}
}
class B {
	public void show() {
		for(int i=0; i<100; i++) {
			System.out.println("Hello");
		}
	}
}

public class ThreadInJava {

	public static void main(String[] args) {

		A a = new A();
		B b = new B();
		a.show();
		b.show();
	}

}

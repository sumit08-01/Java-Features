package com.core_java;

class Employee{
	String name;
	int age;
	
	//Static Block
	{
		name = "sumit";
		age = 21;
	}
}

public class StaticKeyWord {

	public static void main(String[] args) {
		Employee employee = new Employee();
		String name = employee.name;
		System.out.println(name);
		
		
	}

}
